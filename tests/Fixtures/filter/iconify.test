{%- apply iconify -%}
<!DOCTYPE html>
<html lang="en">

    <head>
        <meta charset="utf-8">
    </head>

    <body>

        <!-- Iconify SVG framework -->
        <span class="iconify" data-icon="mdi:home"></span>
        <span class="iconify-inline" data-icon="mdi:home"></span>

        <!-- Iconify Icon Web Component -->
        <iconify-icon icon="mdi:home"></iconify-icon>
        <Icon icon="mdi:home" />

    </body>
</html>
{%- endapply -%}
